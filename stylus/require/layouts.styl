/*------------------------------------*\
    STRUCTURAL ELEMENTS

    for default standard elements, 
    such as .clear, see style.css
\*------------------------------------*/

.wrapper
    max-width 1280px
    width 95%
    margin 0 auto
    position relative

/*------------------------------------*\
    PARTNERS LIST
\*------------------------------------*/

#wt_full-partners-list
    list-style-type none
    margin 0
    padding 0
    display grid
    grid-template-columns repeat(auto-fit, minmax(200px, 1fr))
    column-gap 2rem
    row-gap 2rem
